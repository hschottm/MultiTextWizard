{
    "name":"hschottm/contao-multitextwizard",
    "description":"contao-multitextwizard is a backend widget that can be used to add an array of text input fields to Contao backend forms.",
    "keywords":["contao", "widget", "extension", "input", "bundle", "wizard", "multiple"],
    "type":"contao-bundle",
    "license":["LGPL-3.0-or-later", "CC-BY-NC-SA-4.0"],
    "authors":[
      {
        "name":"Helmut Schottm√ºller",
        "homepage":"https://github.com/hschottm"
      }
    ],
    "require":{
      "php": "^8.1",
      "contao/core-bundle": "^5.0",
      "contao-community-alliance/composer-plugin":"~3.2"
    },
    "conflict": {
          "contao/manager-plugin": "<2.0 || >=3.0"
      },
    "autoload": {
          "psr-4": {
              "Hschottm\\MultiTextWizardBundle\\": "src/"
          },
      "classmap": [
            "src/Resources/contao/"
            ],
        "exclude-from-classmap": [
        "src/Resources/contao/config/",
        "src/Resources/contao/languages/"
            ]
      },
      "extra":{
        "contao-manager-plugin": "Hschottm\\MultiTextWizardBundle\\ContaoManager\\Plugin"
      },
      "support": {
        "issues": "https://github.com/hschottm/MultiTextWizard/issues",
        "source": "https://github.com/hschottm/MultiTextWizard"
    }
  }
  